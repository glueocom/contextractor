{
    "extends": "@apify/tsconfig",
    "compilerOptions": {
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "target": "ES2022",
        "outDir": "dist",
        "rootDir": "src",
        "resolveJsonModule": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": ["src/**/*"]
}
