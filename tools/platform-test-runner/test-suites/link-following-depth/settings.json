{
    "waitUntil": "DOMCONTENTLOADED",
    "saveRawHtmlToKeyValueStore": false,
    "saveExtractedMarkdownToKeyValueStore": true,
    "saveExtractedTextToKeyValueStore": false,
    "saveExtractedJsonToKeyValueStore": false,
    "extractionMode": "BALANCED",
    "maxRequestRetries": 3,
    "pageLoadTimeoutSecs": 60,
    "maxPagesPerCrawl": 8,
    "maxCrawlingDepth": 2,
    "linkSelector": "article a, .content a, main a"
}
